            android:layout_height="wrap_content"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/leftpersonality"
                android:layout_width="60dp"
                android:layout_height="match_parent"
                android:contentDescription="@string/goback"
                android:src="@drawable/backcartoon" />

            <TextView
                android:id="@+id/textviewpersonality"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/personality"
                android:textColor="#ffffff"
                android:textSize="28sp" />

            <ImageView
                android:id="@+id/starcorrect"
                android:layout_width="40dp"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:contentDescription="@string/star"
                android:paddingLeft="3dp"
                android:paddingRight="3dp"
                android:src="@drawable/starcartoon" />

            <TextView
                android:id="@+id/solvedText"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:text="@string/zero"
                android:textColor="@color/white"
                android:textSize="30sp" />
        </LinearLayout>
        </LinearLayout>
    </android.support.v7.widget.Toolbar>

